{
  "name": "vscode-snippets",
  "description": "Visual Studio Code snippets created by Gabriel Masson.",
  "version": "1.0.2",
  "displayName": "VSCode Snippets",
  "publisher": "gmass0n",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gmass0n/vscode-snippets"
  },
  "scripts": {
    "vscode:publish": "vsce package && vsce publish"
  },
  "engines": {
    "vscode": "0.10.x"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets-ts.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^1.88.0"
  }
}
